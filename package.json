    "browserify-zlib": "^0.1.4",
    "buffer": "^4.9.1",
    "classnames": "^2.2.6",
    "console-browserify": "^1.1.0",
    "constants-browserify": "^1.0.0",
    "date-fns": "^2.0.0-alpha.25",
    "dns.js": "^1.0.1",
    "domain-browser": "^1.2.0",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^6.0.0",
    "events": "^1.1.1",
    "https-browserify": "0.0.1",
    "mobx": "^5.6.0",
    "mobx-react": "^5.4.2",
    "moment": "^2.22.2",
    "nanocurrency": "^2.0.4",
    "path-browserify": "0.0.0",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "react": "16.6.3",
    "react-native": "0.59.0",
    "react-native-animatable": "^1.3.0",
    "react-native-camera": "^1.4.3",
    "react-native-crypto": "^2.1.2",
    "react-native-fingerprint-scanner": "^2.5.0",
    "react-native-gesture-handler": "^1.0.10",
    "react-native-level-fs": "^3.0.1",
    "react-native-localization": "^2.0.3",
    "react-native-os": "^1.2.1",
    "react-native-paper": "^2.2.8",
    "react-native-qrcode": "^0.2.7",
    "react-native-qrcode-scanner": "^1.1.0",
    "react-native-randombytes": "^3.5.1",
    "react-native-side-menu": "^1.1.3",
    "react-native-status-bar-height": "^2.2.0",
    "react-native-storage": "^0.2.3",
    "react-native-tcp": "^3.3.0",
    "react-native-udp": "^2.3.1",
    "react-native-vector-icons": "^6.1.0",
    "react-navigation": "^3.0.2",
    "readable-stream": "^1.0.33",
